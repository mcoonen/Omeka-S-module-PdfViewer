{
    "name": "daniel-km/omeka-s-module-pdf-viewer",
    "description": "Pdf Viewer (module for Omeka S)",
    "type": "library",
    "license": "CECILL-2.1",
    "authors": [
        {
            "name": "Daniel Berthereau",
            "email": "Daniel.packagist@Berthereau.net"
        }
    ],
    "homepage": "https://gitlab.com/Daniel-KM/Omeka-S-module-PdfViewer",
    "support": {
        "issues": "https://gitlab.com/Daniel-KM/Omeka-S-module-PdfViewer/-/issues",
        "forum": "https://forum.omeka.org/"
    },
    "keywords": [
        "Omeka S",
        "Omeka S module",
        "viewer",
        "pdf"
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "mozilla/pdfjs",
                "version": "5.4.54",
                "source": {
                    "url": "https://github.com/mozilla/pdf.js",
                    "type": "git",
                    "reference": "master"
                },
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/mozilla/pdf.js/releases/download/v5.4.54/pdfjs-5.4.54-dist.zip",
                    "reference": "v5.4.54"
                }
            }
        }
    ],
    "require": {
        "slowprog/composer-copy-file": "~0.3",
        "mozilla/pdfjs": "^5.4.54"
    },
    "autoload": {
        "psr-4": {
            "PdfViewer\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "SlowProg\\CopyFile\\ScriptHandler::copy",
            "@php -f 'data/scripts/build.php'"
        ],
        "post-update-cmd": [
            "SlowProg\\CopyFile\\ScriptHandler::copy",
            "@php -f 'data/scripts/build.php'"
        ]
    },
    "extra": {
        "copy-file": {
            "vendor/mozilla/pdfjs/": "asset/vendor/pdf.js/"
        }
    }
}
